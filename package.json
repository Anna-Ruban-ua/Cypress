{
    "name": "task3_cypress",
    "version": "1.0.0",
    "description": "Telnyx Cypress E2E Testing Project",
    "main": "index.js",
    "scripts": {
        "test": "cypress run",
        "test:chrome": "cypress run --browser chrome",
        "test:firefox": "cypress run --browser firefox",
        "test:mobile": "cypress run --config-file cypress/configs/cypress.config.mobile.ts",
        "test:fullscreen": "cypress run --config-file cypress/configs/cypress.config.fullscreen.ts",
        "open": "cypress open",
        "test:record": "cypress run --record"
    },
    "author": "Anna Ruban",
    "devDependencies": {
        "cypress": "^14.2.0",
        "typescript": "^5.8.2"
    }
}
